{
  "name": "brin-client-count",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "docker:dev": "docker-compose --profile dev up --build",
    "docker:prod": "docker-compose --profile prod up --build",
    "docker:hub": "docker-compose --profile hub up",
    "docker:hub-dev": "docker-compose --profile hub-dev up",
    "docker:build": "docker build -t brin-client-count .",
    "docker:build-dev": "docker build -f Dockerfile.dev -t brin-client-count:dev .",
    "docker:run": "docker run -p 3000:3000 brin-client-count",
    "docker:run-dev": "docker run -p 3000:3000 -v $(pwd):/app -v /app/node_modules brin-client-count:dev",
    "docker:push": "docker push nojinnojs/brin-client-count:latest && docker push nojinnojs/brin-client-count:dev",
    "docker:tag": "docker tag brin-client-count nojinnojs/brin-client-count:latest && docker tag brin-client-count:dev nojinnojs/brin-client-count:dev",
    "docker:clean": "docker system prune -f && docker volume prune -f"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-day-picker": "^9.11.0",
    "react-dom": "19.1.0",
    "recharts": "^2.15.4",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5"
  }
}
